# --- Provider Logic ---
LLM_PROVIDER=ollama
LLM_MODEL=qwen2.5-coder:7b
LLM_ENDPOINT=http://localhost:11434/v1
LLM_API_KEY=ollama

# --- Hybrid Embedding Logic ---
EMBEDDING_PROVIDER=ollama
EMBEDDING_MODEL=qwen3-embedding:0.6b
EMBEDDING_ENDPOINT=http://localhost:11434/api/embeddings
EMBEDDING_DIMENSIONS=1024
HUGGINGFACE_TOKENIZER=Qwen/Qwen2.5-Coder-7B

# --- RAG Tuning ---
CHUNK_SIZE=2048
CHUNK_OVERLAP=256

# --- System ---
ENABLE_BACKEND_ACCESS_CONTROL=false
SYSTEM_ROOT_DIRECTORY=D:/Development/ALL_COGNEE_MEMORIES/agentic_env/.cognee_system
DATA_ROOT_DIRECTORY=D:/Development/ALL_COGNEE_MEMORIES/agentic_env/.data_storage
COGNEE_LOGS_DIR=D:/Development/ALL_COGNEE_MEMORIES/agentic_env/logs

# debugging
OTEL_SDK_DISABLED=true
TELEMETRY_DISABLED=true

# Ensure this is set for Cognee's search routing
SEARCH_TYPE=HYBRID